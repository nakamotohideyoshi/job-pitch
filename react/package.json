  "name": "react-redux-universal-hot-example",
  "description": "Example of an isomorphic (universal) webapp using react redux and hot reloading",
  "author": "Erik Rasmussen <rasmussenerik@gmail.com> (http://github.com/erikras)",
  "license": "MIT",
  "version": "0.9.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/erikras/react-redux-universal-hot-example"
  },
  "homepage": "https://github.com/erikras/react-redux-universal-hot-example",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel"
  ],
  "main": "bin/server.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "build-dlls": "webpack --colors --display-error-details --config webpack/vendor.config.js",
    "build": "better-npm-run build",
    "build-local": "better-npm-run build-local",
    "build-all": "concurrently \"npm run build-local\" \"npm run build\""
  },
  "betterScripts": {
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "WEBPACK_DLLS": 1
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "WEBPACK_DLLS": 1
      }
    },
    "build": {
      "command": "webpack --colors --display-error-details --config=webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-local": {
      "command": "webpack --colors --display-error-details --config=webpack/prod_local.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
